<!DOCTYPE html>
<html>
  <head>
    <title>Overtime</title>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
    <%= csrf_meta_tags %>
  </head>

  <body>
    <div class="container">
      <div class="row">
        <div class="col">
        <%= gflash %>
          <%= render 'shared/nav' %>
          <%= yield %>
        </div>
      </div>
    </div>
    <%#= js add_gritter(flash[:notice], :title => "App Notification!", :sticky => false, :position => :bottom_right) %>
  </body>
</html>
